{
  "version": 2,
  "name": "konga-sensei-typing-game",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/.*\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}